# CONFIGURAÇÃO ESPECÍFICA PARA SERVBAY
# Sistema Importa Precifica v2.0

# Configurações do Banco de Dados ServBay
DB_HOST=127.0.0.1
DB_NAME=importa_precificacao
DB_USER=root
DB_PASS=
DB_CHARSET=utf8mb4

# Configurações da API ServBay
API_BASE_URL=http://localhost:8080/importa-precifica/api/
API_VERSION=v1
DEBUG=true

# Configurações de Upload ServBay
UPLOAD_MAX_SIZE=52428800
UPLOAD_DIR=../uploads/xml/
ALLOWED_EXTENSIONS=xml

# Configurações de Cache ServBay
CACHE_ENABLED=true
CACHE_TTL=3600

# Configurações de Log ServBay
LOG_LEVEL=INFO
LOG_FILE=../logs/api.log

# Configurações de Segurança ServBay
API_KEY=
CORS_ORIGIN=*
RATE_LIMIT=100

# Configurações específicas ServBay
SERVBAY_ENV=development
SERVBAY_PORT=8080
SERVBAY_WEB_ROOT=/importa-precifica